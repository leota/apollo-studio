{
  "org": [
    {
      "id": "bff-foobar-1",
      "name": "BFF FooBar 13",
      "targetDomain": "https://api.foobar.com",
      "defaults": {
        "port": 3000,
        "addr": "localhost"
      },
      "graphql": {
        "path": "/graphql"
      },
      "rest": {
        "path": "/api"
      },
      "views": {
        "src": "__root/server/views/",
        "engine": "ejs"
      },
      "cors": {
        "acceptedDomains": [
          "*"
        ],
        "methods": [
          "GET",
          "HEAD",
          "PUT",
          "PATCH",
          "POST",
          "DELETE"
        ],
        "preflightContinue": false,
        "optionsSuccessStatus": 200
      },
      "remotes": {
        "getCookies": "https://cookiefarm.com/cookies",
        "getBread": "https://breadcorp.com/bread"
      }
    }
  ]
}